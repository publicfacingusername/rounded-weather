{
	// Worker name
	"name": "weather-proxy",
  
	// Required on every publish
	"compatibility_date": "2025-05-06",
  
	// Default: deploy to <name>.<account>.workers.dev
	"workers_dev": false,
  
	// Routes array
	"routes": [
		{
		"pattern": "roundedweather.com/*",
		"zone_name": "roundedweather.com"
		}
	],

	// Entry point to the Worker script
	"main": "src/index.ts"
}
  